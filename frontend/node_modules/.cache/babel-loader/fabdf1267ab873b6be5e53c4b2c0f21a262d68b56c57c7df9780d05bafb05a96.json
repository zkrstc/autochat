{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport axios from 'axios';\nexport default {\n  name: 'ModulesPage',\n  data() {\n    return {\n      requirements: [],\n      selectedRequirementId: '',\n      modules: [],\n      selectedModuleCode: ''\n    };\n  },\n  mounted() {\n    this.fetchRequirements();\n  },\n  methods: {\n    async fetchRequirements() {\n      const res = await axios.get('/api/requirements/list');\n      this.requirements = res.data;\n    },\n    async onRequirementChange() {\n      const res = await axios.post('/api/modules/generate', {\n        requirement_id: this.selectedRequirementId\n      });\n      this.modules = res.data;\n      this.selectedModuleCode = '';\n    },\n    onModuleChange(event) {\n      const moduleId = parseInt(event.target.value);\n      const mod = this.modules.find(m => m.id === moduleId);\n      this.selectedModuleCode = mod ? mod.code : '// 无模块代码';\n    },\n    copyCode() {\n      navigator.clipboard.writeText(this.selectedModuleCode);\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","requirements","selectedRequirementId","modules","selectedModuleCode","mounted","fetchRequirements","methods","res","get","onRequirementChange","post","requirement_id","onModuleChange","event","moduleId","parseInt","target","value","mod","find","m","id","code","copyCode","navigator","clipboard","writeText"],"sources":["C:\\Users\\ASUS\\Desktop\\autochat\\frontend\\src\\views\\ModulesPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"p-8\">\r\n\r\n    <div class=\"flex items-center justify-between mb-6\">\r\n      <div class=\"w-1/2 pr-4\">\r\n        <label class=\"block text-gray-700 mb-2\">需求选择：</label>\r\n        <select v-model=\"selectedRequirementId\" @change=\"onRequirementChange\"\r\n          class=\"border border-gray-300 rounded-button px-4 py-2 min-w-[200px] focus:outline-none focus:ring-2 focus:ring-primary\">\r\n          <option disabled value=\"\">请选择需求</option>\r\n          <option v-for=\"req in requirements\" :value=\"req.id\" :key=\"req.id\">\r\n            {{ req.name }} v{{ req.version }}\r\n          </option>\r\n        </select>\r\n      </div>\r\n      <div class=\"w-1/2 pl-4\">\r\n        <label class=\"block text-gray-700 mb-2\">模块选择：</label>\r\n        <select v-model=\"selectedModuleName\" @change=\"onModuleChange\"\r\n          class=\"border border-gray-300 rounded-button px-4 py-2 min-w-[200px] focus:outline-none focus:ring-2 focus:ring-primary\">\r\n          <option disabled value=\"\">请选择模块</option>\r\n          <option v-for=\"mod in modules\" :value=\"mod.name\" :key=\"mod.id\">{{ mod.name }}</option>\r\n        </select>\r\n      </div>\r\n    </div>\r\n\r\n\r\n\r\n    <div class=\"bg-white rounded-lg shadow p-6\">\r\n      <div class=\"flex items-center justify-between mb-4\">\r\n        <div class=\"flex items-center\">\r\n          <i class=\"fas fa-code text-primary text-xl\"></i>\r\n          <h3 class=\"ml-2 text-lg font-medium\">代码预览</h3>\r\n        </div>\r\n        <div class=\"flex space-x-2\">\r\n          <button @click=\"copyCode\" class=\"px-4 py-2 bg-primary text-white !rounded-button\">\r\n            <i class=\"far fa-copy mr-2\"></i>复制代码\r\n          </button>\r\n          <button class=\"px-4 py-2 border border-gray-300 !rounded-button\">\r\n            <i class=\"fas fa-download mr-2\"></i>生成代码\r\n          </button>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 代码标签页 -->\r\n      <div class=\"mb-4 border-b border-gray-200\">\r\n        <ul class=\"flex flex-wrap -mb-px\">\r\n          <li v-for=\"(code, index) in moduleCodes\" :key=\"index\">\r\n            <button @click=\"activeTab = index\"\r\n              :class=\"{ 'text-primary border-primary': activeTab === index, 'text-gray-500 hover:text-gray-600 hover:border-gray-300': activeTab !== index }\"\r\n              class=\"inline-block p-4 border-b-2 rounded-t-lg\">\r\n              {{ code.language || `代码片段 ${index + 1}` }}\r\n            </button>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n\r\n      <!-- 代码显示区域 -->\r\n      <pre v-if=\"moduleCodes.length > 0\" id=\"codeBlock\" class=\"...\" style=\"height: 600px;\">\r\n        {{ moduleCodes[activeTab]?.code || '// 无代码内容' }}\r\n      </pre>\r\n      <pre v-else class=\"...\" style=\"height: 600px;\">\r\n        // 请选择需求和模块查看代码\r\n      </pre>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'ModulesPage',\r\n  data() {\r\n    return {\r\n      requirements: [],\r\n      selectedRequirementId: '',\r\n      modules: [],\r\n      selectedModuleCode: '',\r\n      \r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchRequirements();\r\n  },\r\n  methods: {\r\n    async fetchRequirements() {\r\n      const res = await axios.get('/api/requirements/list');\r\n      this.requirements = res.data;\r\n    },\r\n    async onRequirementChange() {\r\n      const res = await axios.post('/api/modules/generate', {\r\n        requirement_id: this.selectedRequirementId\r\n      });\r\n      this.modules = res.data;\r\n      this.selectedModuleCode = '';\r\n    },\r\n    onModuleChange(event) {\r\n      const moduleId = parseInt(event.target.value);\r\n      const mod = this.modules.find(m => m.id === moduleId);\r\n      this.selectedModuleCode = mod ? mod.code : '// 无模块代码';\r\n    },\r\n    copyCode() {\r\n      navigator.clipboard.writeText(this.selectedModuleCode);\r\n    }\r\n  }\r\n\r\n}\r\n\r\n</script>"],"mappings":";;AAmEA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,EAAE;MAChBC,qBAAqB,EAAE,EAAE;MACzBC,OAAO,EAAE,EAAE;MACXC,kBAAkB,EAAE;IAEtB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,iBAAiB,CAAC,CAAC;EAC1B,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,iBAAiBA,CAAA,EAAG;MACxB,MAAME,GAAE,GAAI,MAAMV,KAAK,CAACW,GAAG,CAAC,wBAAwB,CAAC;MACrD,IAAI,CAACR,YAAW,GAAIO,GAA